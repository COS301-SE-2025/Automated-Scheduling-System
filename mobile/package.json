    "expo": "~54.0.0",
    "expo-font": "~14.0.8",
    "expo-modules-core": "~3.0.17",
    "expo-router": "~6.0.7",
    "expo-secure-store": "^15.0.7",
    "react": "^19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
